{
  "extends": [
    "plugin:flowtype/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["flowtype", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "linebreak-style": 0,
    "rules": {
      "linebreak-style": ["error", "unix"]
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
